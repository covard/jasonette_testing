{
  "$jason": {
    "head": {
      "title": "",
      "description": "Handle links and render",
      "actions": {
        "$load": {
          "type": "$render"
        },
        "visit": {
          "type": "$default"
        }
      },
      "templates": {
        "body": {
          "header": {
            "type": "label",
            "text": "FundMe",
            "style": {
              "background": "#172727",
              "color": "#FFFFFF"
            }
          },
          "background": {
            "type": "html",
            "url": "https://www.fundme.com/en",
            "action": {
              "trigger": "visit"
            }
          },
          "footer": {
            "tabs": {
              "style": {
                "background": "rgba(255,255,255,0.8)",
                "color": "#009efa"
              },
              "items": [{
                "image": "https://raw.githubusercontent.com/CrowdEngine/icons/master/search.png",
                "text": "Explore",
                "style": {
                  "height": "21"
                },
                "url": "https://fundme.com/en/explore"
            },
            {
              "image": "https://raw.githubusercontent.com/CrowdEngine/icons/master/dashboard.png",
              "text": "Dashboard",
              "style": {
                "height": "21"
              },
              "url": "https://fundme.com/en/users/dashboard"
            }]
          }
        }
      }
    }
  }
}
