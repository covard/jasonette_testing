{
  "$jason": {
    "head": {
      "title": "CrowdEngine",
      "description": "Handle links and render",
      "actions": {
        "$load": {
          "type": "$render"
        },
        "visit": {
          "type": "$default"
        }
      },
      "templates": {
        "body": {
          "header": {
            "type": "label",
            "text": "FundMe",
            "style": {
              "background": "#ccc",
              "color": "#000"
            }
          },
          "background": {
            "type": "html",
            "url": "https://www.fundme.com/en",
            "action": {
              "trigger": "visit"
            }
          }
        }
      }
    }
  }
}
